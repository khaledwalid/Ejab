@model Ejab.BAL.ModelViews.UserViewModel

@{
    ViewBag.Title = "AddServiceProvider";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-8">
        <h2>المستخدمين</h2>
        <ol class="breadcrumb">
            <li>
                <a href="index.html">الرئيسية </a>
            </li>
            <li>
                اضافه طالب  خدمه
            </li>
        </ol>
    </div>
    <div class="col-sm-4 text-left">

    </div>
</div>



<div class="row">
    <div class="col-lg-12">
        <div class="wrapper wrapper-content">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">

                        <div class="ibox-content">
                            <div class="well well-sm">
                                <div class="row">
                                   @using (Html.BeginForm("AddRequester", "Customer", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
                                   {
                                    @Html.AntiForgeryToken()
                                    @Html.ValidationSummary()
                                    <div class="col col-sm-12 ">
                                        <div class="form-group ">
                                            <label class="col-lg-2 control-label">First name</label>
                                            <div class="col-lg-8">
                                                @Html.TextBoxFor(x => x.FirstName, new { @class = "form-control" })
                                                @Html.ValidationMessageFor(x => x.FirstName, "*")
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-lg-2 control-label">Last Name</label>
                                            <div class="col-lg-8">
                                                @Html.TextBoxFor(x => x.LastName, new { @class = "form-control" })
                                                @Html.ValidationMessageFor(x => x.LastName, "*")
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-lg-2 control-label">Mobile</label>
                                            <div class="col-lg-8">
                                                @Html.TextBoxFor(x => x.Mobile, new { @class = "form-control", @type = "number" })
                                                @Html.ValidationMessageFor(x => x.Mobile, "*")
                                            </div>
                                        </div>

                                        <div class="form-group ">
                                            <label class="col-lg-2 control-label">Email</label>
                                            <div class="col-lg-8">
                                                @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
                                                @Html.ValidationMessageFor(x => x.Email, "*")
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label class="col-lg-2 control-label">PassWord </label>
                                            <div class="col-lg-8">
                                                @Html.PasswordFor(x => x.Password, new { @class = "form-control" })
                                                @Html.ValidationMessageFor(x => x.Password, "*")
                                            </div>
                                        </div>
                                        <div class="form-group ">
                                            <label class="col-lg-2 control-label">Profile Image</label>



                                            <div class="col-lg-8">
                                                <input type="file" name="file" class="form-control" id="Profileupload" onchange="ImagePreview(this);" />
                                                <img alt="ImagePreview" id="ImgPreview" class="pull-right "  />

                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="col-lg-4">
                                                <button type="submit" class="btn btn-w-m btn-success  ">Save</button>
                                            </div>
                                        </div>

                                    </div>

                                   }


                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>


</div>















































