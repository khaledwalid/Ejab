
@{
    ViewBag.Title = "DownloadServiceProviderViewPDF";
    // Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<style>
    table {
        overflow: visible !important;
    }

    thead {
        display: table-header-group;
    }

    tfoot {
        display: table-row-group;
    }

    tr {
        page-break-inside: avoid;
    }
</style>

<!doctype html>
<h2 style="text-align:center">@Resources.Global.Customers</h2>
<table class="table table-striped" style="table-layout: fixed; width: 100% ;margin:5px; padding:100px;">
    <thead>
        <tr>
            <th>@Resources.Global.FirstName  </th>
            <th>@Resources.Global.LastName   </th>

            <th>@Resources.Global.Email   </th>
            <th>@Resources.Global.Mobile   </th>
        </tr>
    </thead>
    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.FirstName   </td>
                <td>@item.LastName    </td>
                <td>@item.Email    </td>
                <td>@item.Mobile </td>
               
                
            </tr>

        }

    </tbody>
</table>




